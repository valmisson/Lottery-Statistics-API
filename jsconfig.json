{
  "compilerOptions": {
    "target": "es2018",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "~/*": ["./*"],
      "@src/*": ["src/*"],
      "@utils/*": ["src/utils/*"],
      "@services/*": ["src/services/*"],
      "@routes/*": ["src/routes/index.js"]
    }
  },
  "exclude": ["node_modules"]
}
